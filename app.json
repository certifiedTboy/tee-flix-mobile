{
  "expo": {
    "name": "tee-flix",
    "slug": "tee-flix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "teeflix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
     "splash": {
      "image": "./assets/cover.png",
      "resizeMode": "contain",
      "backgroundColor": "#fcd834"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#fcd834",
        "foregroundImage": "./assets/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.certifiedtboy.teeflix"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#fcd834",
          "image": "./assets/cover.png",
          "dark": {
            "image": "./assets/cover.png",
            "backgroundColor": "#fcd834"
          },
          "imageWidth": 300
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c3ca5440-3887-4030-a464-06da2cb75a43"
      }
    }
  }
}
